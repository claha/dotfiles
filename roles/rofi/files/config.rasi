configuration {
    modes: "drun,run,ssh,window,combi";
    combi-modes: "run,drun";
    combi-hide-mode-prefix: true;
    display-combi: "run";
    font: "TerminessNerdFontMono 14";
    fixed-num-lines: false;
    terminal: "alacritty";
    disable-history: false;
    matching: "fuzzy";
}

@theme "theme"
